            "name": "parameters",
            "required": true,
            "description": "create test result request body",
            "schema": {
              "type": "string"
            }
          }
        ],
        "externalDocs": {
          "description": "TBD",
          "url": "https://wiki.saucelabs.com"
        },
        "tags": [
          "Job"
        ],
        "responses": {
          "200": {
            "description": "create test result",
            "content": {
              "application/json": {
                "schema": {
                  "type": "json"
                }
              }
            }
          }
        }
      }
    }
  }
